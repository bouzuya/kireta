rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /check_lists/{check_list_id} {
      allow read, write: if true;
    }

    match /repositories/{repository_id} {
      allow read, write: if true;
    }
    match /users/{user_id} {
      allow read, write: if true;
    }
    match /transactions/{transaction_id} {
      allow read, write: if true;
    }
  }
}
